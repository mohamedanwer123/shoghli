<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cm.mywork">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activites.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activites.Signup" />
        <activity
            android:name=".Activites.Home"
            android:label="@string/title_activity_home" />
        <activity android:name=".Activites.UpdateUserData" />
        <activity android:name=".Activites.AddClients" />
        <activity android:name=".Activites.Client_Info" />
        <activity android:name=".Activites.AddBills" />
        <activity android:name=".Activites.ShowBills" />
        <activity android:name=".Activites.AddProduct"></activity>
    </application>

</manifest>